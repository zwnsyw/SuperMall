A,KAAA;AAaA,GA9BA;AA+BA;AACA,EAAA,OAhCA,qBAgCA;AACA;AACA,SAAA,gBAAA,GAFA,CAGA;;AACA,SAAA,YAAA,CAAA,KAAA;AACA,SAAA,YAAA,CAAA,KAAA;AACA,SAAA,YAAA,CAAA,MAAA,EANA,CAQA;AACA;AACA;AACA;AACA;AACA;AAEA;AAEA,GAjDA;AAmDA,EAAA,QAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,aAAA,KAAA,KAAA,CAAA,KAAA,WAAA,EAAA,IAAA;AACA;AAHA,GAnDA;AAwDA,EAAA,OAxDA,qBAwDA;AACA;AACA;AACA,QAAA,OAAA,GAAA,QAAA,CAAA,KAAA,KAAA,CAAA,MAAA,CAAA,OAAA,EAAA,GAAA,CAAA;AAEA,SAAA,IAAA,CAAA,GAAA,CAAA,eAAA,EAAA,YAAA;AACA;AACA;AACA;AACA,MAAA,OAAA;AACA,KALA,EALA,CAWA;AACA;AACA;;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,KAAA,CAAA,UAAA,CAAA,GAAA,CAAA,SAAA;AACA,GAvEA;AAwEA,EAAA,OAAA;AACA;;;AAIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAGA,IAAA,QAjBA,oBAiBA,KAjBA,EAiBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,cAAA,KAAA;AACA,aAAA,CAAA;AACA,eAAA,WAAA,GAAA,KAAA;AACA;;AACA,aAAA,CAAA;AACA,eAAA,WAAA,GAAA,KAAA;AACA;;AACA,aAAA,CAAA;AACA,eAAA,WAAA,GAAA,MAAA;AACA;AATA;AAWA,KA9BA;AA+BA,IAAA,SA/BA,uBA+BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EADA,CAEA;;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,CAAA,EAAA,GAAA;AACA,KAnCA;AAqCA,IAAA,aArCA,yBAqCA,QArCA,EAqCA;AACA;AACA;AACA,WAAA,aAAA,GAAA,CAAA,QAAA,CAAA,CAAA,GAAA,GAAA;AACA,KAzCA;AA2CA,IAAA,QA3CA,sBA2CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,YAAA,CAAA,KAAA,WAAA,EAFA,CAGA;AACA;AACA;AAhDA,+DAkDA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,MAAA,GACA,KAAA,YAAA,CAAA,KAAA,WAAA,CADA;AAEA,GArDA,6EA+DA;AAAA;;AACA;AACA,IAAA,iBAAA,GAAA,IAAA,CAAA,UAAA,MAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,IAAA;AACA,MAAA,KAAA,CAAA,UAAA,GAAA,MAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA;AAEA,KALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA,CACA,CANA;AAQA,GAzEA,mEA0EA,IA1EA,EA0EA;AAAA;;AACA,QAAA,IAAA,GAAA,KAAA,KAAA,CAAA,IAAA,EAAA,IAAA,GAAA,CAAA;;AACA,IAAA,aAAA,CAAA,IAAA,EAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AAAA;;AACA,+BAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA,IAAA,EAAA,IAAA,iDAAA,GAAA,CAAA,IAAA,CAAA,IAAA;;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA,IAAA,IAAA,CAAA,CAFA,CAIA;;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,YAAA,GALA,CAOA;;AACA,KARA;AASA,GArFA;AAxEA,CAAA","sourcesContent":["<template>\n  <div id=\"home\">\n  <NavBar class='home-nav'>\n    <div slot=\"center\">世白么购物街</div>\n  </NavBar>\n\n  <Scroll class=\"content\" \n  ref='scroll' \n  :probe-type='3' \n  @scroll=\"contentScroll\"\n  :pullUpload=\"true\"\n  @pullingUp = \"loadMore\"\n  >\n\n  <HomeSwiper :banners = 'banners'  @/>\n  <RecommendView :recommends = \"recommends\" />\n  <FeatureView/>\n  <TabControl  class=\"tab-control\" :titles=\"['流行','新款','精选']\" @tabClick=\"tabClick\" ref='tabControl'/>\n  <GoodsList  :goods = \"showGoods\" />\n</Scroll>\n\n<BackTop @click.native='backClick' v-show=\"isShowBackTop\"></BackTop >\n  </div>\n</template>\n\n<script>\nimport NavBar from 'components/common/navbar/NavBar';\nimport TabControl from 'components/content/tabControl/TabControl';\nimport GoodsList from 'components/content/goods/GoodsList';\nimport Scroll from 'components/common/scroll/Scroll';\nimport BackTop from 'components/content/backTop/BackTop'\n\nimport HomeSwiper from './childComps/HomeSwiper';\nimport RecommendView from './childComps/RecommendView';\nimport FeatureView from './childComps/FeatureView'\n\nimport {getHomeMultidata,getHomeGoods} from 'network/home';\nimport {debounce} from 'common/utiles'\n\n\n  export default {\n    name: \"Home\",\n    components: {\n      //  公共组件\n    NavBar,\n    TabControl,\n    GoodsList,\n    Scroll,\n    BackTop,\n\n\n    HomeSwiper,\n    RecommendView,\n    FeatureView,\n    \n    },\n    data() {\n        return {\n          banners: [],\n          recommends: [],\n          isShowBackTop: false,\n          tabOffsetTop: 0,\n          // 数据模型\n          goods:{\n            'pop': {page: 0, list: []},\n            'new': {page: 0, list: []},\n            'sell': {page: 0, list: []}\n          },\n          currentType: 'pop'\n        }\n      },\n    //  1. 创建好就请求数据\n    created() {\n      // 加this调用的是methods的函数\n      this.getHomeMultidata();\n      // 2. 请求商品数据\n      this.getHomeGoods(\"pop\");\n      this.getHomeGoods(\"new\");\n      this.getHomeGoods(\"sell\");\n\n      // 3. 组件一旦创建完成就监听事件总线里面的itemImageLoad（图片加载完成事件）然后刷新scrollHeight\n      // 不建议在create里面，推荐在mounted里面\n      // this.$bus.$on(\"itemImageLoad\", ()=>{\n      //   console.log('itemImageLoad');\n      //   // this.$refs.scroll.scroll.refresh\n      //   this.$refs.scroll.refresh()\n\n      // })\n      \n    },\n    \n    computed: {\n      showGoods(){\n        return this.goods[this.currentType].list\n      }\n    },\n    mounted() {\n      // 1. 组件一旦创建完成并挂载就监听事件总线里面的itemImageLoad（图片加载完成事件）然后刷新scrollHeight\n      //  const refresh = this.debounce(this.$refs.scroll.refresh,100)\n       const refresh = debounce(this.$refs.scroll.refresh,100)\n\n       this.$bus.$on(\"itemImageLoad\", ()=>{\n        // console.log('itemImageLoad');\n        // this.$refs.scroll.scroll.refresh\n        // this.$refs.scroll.refresh()\n        refresh()\n      })\n      // 2. 获取tabControl的offsetTop\n      // 所有的组件都有一个属性$el  用于获取组件中的元素\n      // this.tabOffsetTop = this.$refs.TabControl.$el.offsetTop\n      console.log(this.$refs.tabControl.$el.offsetTop)\n    },\n    methods: {\n      /**\n       * 事件监听相关\n       */\n\n      // //  防抖函数\n      // debounce(func, delay){\n      //   let timer = null\n      //   return function(...args){\n      //     if (timer) clearTimeout(timer)\n      //     timer = setTimeout(() => {\n      //       func.apply(this,args)\n      //     }, delay);\n      //   }\n      // },\n      \n\n      tabClick(index){\n        console.log(\"0\")\n        switch(index){\n          case 0:\n            this.currentType = \"pop\";\n            break;\n          case 1:\n            this.currentType = 'new';\n            break;\n          case 2:\n            this.currentType = \"sell\";\n            break\n        }\n      },\n      backClick(){\n      console.log(\"回到顶部\");\n      // this.$refs.scroll.scroll.scrollTo(0,0,500);\n      this.$refs.scroll.scrollTo(0,0,500);\n    },\n\n    contentScroll(position){\n      // console.log(position)\n      // position.y > 1000px\n      this.isShowBackTop = (-position.y) > 550\n    },\n\n    loadMore(){\n      console.log(\"上拉加载更多\")\n      this.getHomeGoods(this.currentType);\n      // 重新计算可滚动区域\n      // this.$refs.scroll.scroll.refresh();\n    },\n\n    loadMore(){\n      console.log('加载更多'),\n      this.getHomeGoods(this.currentType)\n    },\n\n\n\n\n\n\n      /**\n       * 网络请求相关\n       */\n      getHomeMultidata(){\n        // 请求多个数据\n      getHomeMultidata().then((result) => {\n        console.log(result)\n        this.banners = result.data.banner.list\n        this.recommends = result.data.recommend.list\n\n      }).catch((err) => {\n      });\n      \n      },\n      getHomeGoods(type){\n        const page = this.goods[type].page + 1\n        getHomeGoods(type,page).then(res => {\n          this.goods[type].list.push(...res.data.list);\n          this.goods[type].page += 1;\n\n        // 完成上拉加载更多，调用一下继续下次上拉加载\n          this.$refs.scroll.finishPullUp()\n\n        // console.log(\"商品数据\",res)\n      })\n      },\n\n    },\n  }\n</script>\n\n<style scoped>\n  #home{\n    padding-top: 44px;\n    /* 100vh是视图窗口的高度 */\n    height: 100vh;\n    position: relative;\n  }\n  .home-nav{\n    background-color: var(--color-tint);\n    color: #fff;\n    position: fixed;\n    left: 0;\n    right: 0;\n    top: 0;\n    z-index: 9;\n  }\n\n  /* .tab-control{\n    /* position: sticky; */\n    /* top: 44px;\n    z-index: 9;\n  } */ \n\n  .content{\n    overflow: hidden;\n    position: absolute;\n    top: 44px;\n    bottom: 49px;\n    left: 0;\n    right: 0;\n  }\n\n  /* .content{\n    height: calc(100% - 44px - 49px);\n    overflow: hidden;\n    margin-top: 44px;\n  } */\n</style>\n"],"sourceRoot":"src/views/home"}]}
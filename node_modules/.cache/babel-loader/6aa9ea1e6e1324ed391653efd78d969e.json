oduct = context.state.carList.find(item => item.iid === payload.iid)\n    // 2. 判断oldProduct\n    if (oldProduct){  // 数量+1\n      // oldProduct.count +=1\n      context.commit(ADD_COUNTER , oldProduct)\n      resolve(\"当前商品数量 + 1\")\n    }else{ // 添加新商品\n      payload.count =1,\n      // context.state.carList.push(payload)\n      context.commit(ADD_TO_CART , payload)\n      resolve('添加购物车成功')\n    }\n    })\n  }\n}"]}]}